                                .append($("<td>").html(o.WR_Baja.toMoney(1)).css("text-align", "right").css("font-weight", "bold"))
                                .append($("<td>").html(o.N_Total.toMoney(1)).css("text-align", "right"))
                                //.append($("<td>").html(o.Neto_Total.toMoney(1)).css("text-align", "right"))
                                .append($("<td>").html(o.WR_Total.toMoney(1) + "%").css("text-align", "right").css("font-weight", "bold"))


