                    $.each(respoonse.totalesUniverso, function (i, o) {

                        if (o.Item != 'Pre-aprobados Final') {
                            $("#bdy_totalesuniversales").append(
                                $("<tr>")
                                    .append($("<td>").html(o.Item))
                                    .append($("<td>").html(o.N_Pensionados.toMoney()).css("text-align", "right"))
                                    .append($("<td>").html(o.P_Pensionados.toMoney() + "%").css("text-align", "right"))
                                    .append($("<td>").html(o.N_Trabajadores.toMoney()).css("text-align", "right"))
                                    .append($("<td>").html(o.P_Trabajadores.toMoney() + "%").css("text-align", "right"))
                                    .append($("<td>").html(o.N_Total.toMoney()).css("text-align", "right"))
                                    .append($("<td>").html(o.P_Total.toMoney() + "%").css("text-align", "right"))
                            );
                        } else {
                            $("#bdy_totalesuniversales").append(
                               $("<tr>")
                                   .append($("<td>").html(o.Item).addClass("charlee-pintado-bold"))
                                   .append($("<td>").html(o.N_Pensionados.toMoney()).addClass("charlee-pintado-bold-ralign"))
