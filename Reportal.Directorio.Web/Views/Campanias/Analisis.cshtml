                                   .append($("<td>").html(o.P_Total.toMoney() + "%").addClass("charlee-pintado-bold-ralign"))
                           );
                            $("#lb_NTotalMes").html(o.N_Total.toMoney());
                            $("#lb_PrcAfConCredito").html(o.P_Total.toMoney() + "%");
