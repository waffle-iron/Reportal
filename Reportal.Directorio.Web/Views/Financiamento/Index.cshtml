                    $("#bodyFinanciamiento > tr.total").append($("<td>").html(e.Tasa.toMoney(2) + '%').css("font-weight", "bold"));
                    //$("#bodyFinanciamiento > tr.total").append($("<td>").html(e.Spred.toMoney(2)).css("font-weight", "bold"));
