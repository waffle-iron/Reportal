                $("#bodyFinanciamientoSpread").append($("<tr class=\"ddirecto\">"));
                $("#bodyFinanciamientoSpread > tr.ddirecto").append($("<th>").html("Deudor Directo").css("text-align", "center").css("background", "#42a5f5 none repeat scroll 0 0").css("color", "#FFFFFF"));
