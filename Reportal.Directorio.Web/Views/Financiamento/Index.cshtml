                            pointFormat: "{point.y:.2f}"
                        },
                        credits: {
                            enabled: false
                        },
                        exporting: {
                            enabled: false
                        },
                        legend: {
                            layout: 'horizontal',
                            align: 'left',
                            verticalAlign: 'top',
                            borderWidth: 0
                        },

                        series: [{

                            name: 'Pensionado',
                            data: SpreadPensionado,
                            tooltip: {
                                pointFormat: 'Spread: <b>{point.y:.2f}%</b><br/>',
                                shared: true

                            },
                        }, {
                            name: 'Trabajador',
                            data: SpreadTrabajador,
                            tooltip: {
                                pointFormat: 'Spread: <b>{point.y:.2f}%</b><br/>',
                                shared: true

                            },
                        },
                        {
                            name: 'Deudor Directo',
                            data: SpreadDeudorD,
                            tooltip: {
                                pointFormat: 'Spread: <b>{point.y:.2f}%</b><br/>',
                                shared: true

                            },
                        },{
                            name: 'Total',
                            data: SpreadTotal,
                            tooltip: {
                                pointFormat: 'Spread: <b>{point.y:.2f}%</b><br/>',
                                shared: true

                            },

                        }]



                    });
                });

                $.SecGetJSON(getReportalApiHost() + "api/FechaActualizacion/2/", function (respoonse) {
                    $("#Leyend_FechaActualizacion2").html(respoonse.ActualizacionLeyenda);
                });
                $.SecGetJSON(getReportalApiHost() + "api/FechaActualizacion/2/", function (respoonse) {
                    $("#Leyend_FechaActualizacion3").html(respoonse.ActualizacionLeyenda);
                });

            });
            $("#slPeriodo3").on("change", function () {
                $("#LinealPlazo").html("");
                $.SecGetJSON(getReportalApiHost() + "api/CreditoFinanciamento/" + $("#slPeriodo3").val(), function (data) {

                    var diasPlazo = new Array();
                    var PlazoPensionado = new Array();
                    var PlazoTrabajador = new Array();
                    var PlazoDeudorDirecto = new Array();
                    var PlazoTotal = new Array();


                    $.each(data.Encabezado, function (i, e) {
                        diasPlazo[i] = e.Mes;
                    });

                    $.each(data.Pensionado, function (i, e) {
                        PlazoPensionado[i] = e.Pplazo;
                    });

                    $.each(data.Trabajador, function (i, e) {
                        PlazoTrabajador[i] = e.Pplazo;
                    });
                    $.each(data.DDirecto, function (i, e) {
                        PlazoDeudorDirecto[i] = e.Pplazo;
                    });
                    $.each(data.Total, function (i, e) {
                        PlazoTotal[i] = e.Pplazo;
                    });

                    $('#LinealPlazo').highcharts({
                        colors: ['#42a5f5', '#8bc34a', '#ec407a', '#ffa726'],
                        title: {
                            text: '',
                            x: -20 //center
                        },
                        subtitle: {
                            text: '',
                            x: -20
                        },
                        xAxis: {
                            categories: diasPlazo
                        },
                        yAxis: {
                            title: {
                                text: ''
                            },
                            plotLines: [{
                                value: 0,
                                width: 1,
                                color: '#808080'
                            }]
                        },
                        tooltip: {
                            format: '{value}'
                        },
                        credits: {
                            enabled: false
                        },
                        exporting: {
                            enabled: false
                        },
                        legend: {
                            layout: 'horizontal',
                            align: 'left',
                            verticalAlign: 'top',
                            borderWidth: 0
                        },
                        series: [{

                            name: 'Pensionado',
                            data: PlazoPensionado,
                            tooltip: {
                            },
                        }, {
                            name: 'Trabajador',
                            data: PlazoTrabajador,
                            tooltip: {
                            },
                        }, {
                            name: 'Deudor Directo',
                            data: PlazoDeudorDirecto,
                            tooltip: {
                            },
                        }, {
                            name: 'Total',
                            data: PlazoTotal,
                            tooltip: {
                            },

                        }]
                    });
                });
                $.SecGetJSON(getReportalApiHost() + "api/FechaActualizacion/2/", function (respoonse) {
                    $("#Leyend_FechaActualizacion2").html(respoonse.ActualizacionLeyenda);
                });
            });

            $("#slPeriodo").trigger("change");
            $('a[data-toggle="tab"]').on('shown.bs.tab', function (e) {

                if ($(this).attr("href") === "#pestanaSpread")
                    $("#slPeriodo2").trigger("change");

                if ($(this).attr("href") === "#pestanaPlazo")
                    $("#slPeriodo3").trigger("change");

            })
        });
    </script>
}
