                $("#bodyFinanciamientoPlazo").append($("<tr class=\"trabajador\">"));
                $("#bodyFinanciamientoPlazo > tr.trabajador").append($("<th>").html("Trabajador").css("text-align", "center").css("background", "#42a5f5 none repeat scroll 0 0").css("color", "#FFFFFF"));
